---
import SectionTitle from "./SectionTitle.astro";
import SkillGrid from "./SkillGrid.astro";
import { AllSkills } from "../lib/SkillStack";
---

<section id="skills" class="w-full min-h-screen px-6 py-4 pt-14 bg-background">
  <SectionTitle title="Skills" />
  <div
    class="Fade_Up flex justify-center items-center w-full max-w-7xl mx-auto py-15"
  >
    <SkillGrid stack={AllSkills} />
  </div>
</section>
